<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Project01</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/cerulean-bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>

 @* <body background="~/Image/Villbk.jpg" bgproperties="fixed"> *@ 

<body>


        <div class="navbar navbar-inverse navbar-fixed-top">

            <div class="container">
                <div class="navbar-header">
                    <img class="img-responsive nav navbar-nav navbar-left " src="~/Image/blabann.jpg" alt="WebPageName" />
                </div>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@

                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">

                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home") </li>




                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">Admin
                                <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Users", "ListUsers", "People")</li>
                                <li>@Html.ActionLink("Items", "ListItems", "Item")</li>
                                <li>@Html.ActionLink("Orders", "ListOrders", "Order")</li>
                                <li>@Html.ActionLink("Order details", "ListOrderDetails", "OrderDetail")</li>
                                <li>@Html.ActionLink("Carts", "ListCarts", "Cart")</li>
                            </ul>
                        </li>



                        <li>@Html.ActionLink("Webshop", "StartShop", "Shop") </li>



                    </ul>
                    @Html.Partial("_inOut")
                </div>


            </div>
        </div>

        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year-@DateTime.Now.Month-@DateTime.Now.Day - Lennart Kråksberg</p>
            </footer>
        </div>

        <script src="~/Scripts/jquery-1.10.2.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/scripts/jquery.unobtrusive-ajax.js"></script>
    </body>
</html>